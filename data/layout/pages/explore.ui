<?xml version="1.0" encoding="UTF-8"?>
<interface>
    <requires lib="gtk" version="4.0" />
    <template class="CatalogueWindowExplore" parent="AdwBin">
        <child>
            <object class="AdwLeaflet" id="leaflet">
                <property name="transition-type">over</property>
                <property name="fold_threshold_policy">natural</property>
                <property name="can_unfold">false</property>
                <child>
                    <object class="GtkScrolledWindow">
                        <property name="hscrollbar-policy">never</property>
                        <property name="min-content-width">900</property>
                        <child>
                            <object class="GtkStack" id="stack">
                                <property name="vexpand">True</property>
                                <property name="width-request">360</property>
                                <property name="hexpand">True</property>
                                <property name="vexpand">True</property>
                                <child>
                                    <object class="GtkStackPage">
                                        <property name="name">featured</property>
                                        <property name="title" translatable="yes">Featured</property>
                                        <property name="icon-name">starred-symbolic</property>
                                        <property name="child">
                                            <object class="AdwClamp">
                                                <property name="maximum-size">1000</property>
                                                <property name="tightening-threshold">600</property>
                                                <property name="child">
                                                    <object class="GtkBox" id="featured_box">
                                                        <property name="halign">center</property>
                                                        <property name="hexpand">False</property>
                                                        <property name="orientation">vertical</property>
                                                        <property name="margin-top">24</property>
                                                        <property name="margin-bottom">36</property>
                                                        <property name="margin-start">12</property>
                                                        <property name="margin-end">12</property>
                                                        <property name="valign">start</property>
                                                        <child>
                                                            <object class="GtkFlowBox" id="featured_flowbox">
                                                                <property name="max-children-per-line">3</property>
                                                                <property name="row-spacing">5</property>
                                                                <property name="column-spacing">5</property>
                                                            </object>
                                                        </child>
                                                    </object>
                                                </property>
                                            </object>
                                        </property>
                                    </object>
                                </child>
                                <child>
                                    <object class="GtkStackPage">
                                        <property name="name">games</property>
                                        <property name="title" translatable="yes">Games</property>
                                        <property name="icon-name">applications-games-symbolic</property>
                                        <property name="child">
                                            <object class="AdwClamp">
                                                <property name="maximum-size">1000</property>
                                                <property name="tightening-threshold">600</property>
                                                <property name="child">
                                                    <object class="AdwPreferencesGroup">
                                                        <property name="margin-top">24</property>
                                                        <property name="title" translatable="yes">Games</property>
                                                        <child>
                                                            <object class="GtkBox" id="games_box">
                                                                <property name="halign">center</property>
                                                                <property name="hexpand">False</property>
                                                                <property name="orientation">vertical</property>
                                                                <property name="margin-bottom">36</property>
                                                                <property name="margin-start">12</property>
                                                                <property name="margin-end">12</property>
                                                                <property name="valign">start</property>
                                                            </object>
                                                        </child>
                                                    </object>
                                                </property>
                                            </object>
                                        </property>
                                    </object>
                                </child>
                                <child>
                                    <object class="GtkStackPage">
                                        <property name="name">develop</property>
                                        <property name="title" translatable="yes">Develop</property>
                                        <property name="icon-name">application-x-addon-symbolic</property>
                                        <property name="child">
                                            <object class="AdwClamp">
                                                <property name="maximum-size">1000</property>
                                                <property name="tightening-threshold">600</property>
                                                <property name="child">
                                                    <object class="AdwPreferencesGroup">
                                                        <property name="margin-top">24</property>
                                                        <property name="title" translatable="yes">Develop</property>
                                                        <child>
                                                            <object class="GtkBox" id="develop_box">
                                                                <property name="halign">center</property>
                                                                <property name="hexpand">False</property>
                                                                <property name="orientation">vertical</property>
                                                                <property name="margin-bottom">36</property>
                                                                <property name="margin-start">12</property>
                                                                <property name="margin-end">12</property>
                                                                <property name="valign">start</property>
                                                            </object>
                                                        </child>
                                                    </object>
                                                </property>
                                            </object>
                                        </property>
                                    </object>
                                </child>
                                <child>
                                    <object class="GtkStackPage">
                                        <property name="name">create</property>
                                        <property name="title" translatable="yes">Create</property>
                                        <property name="icon-name">applications-graphics-symbolic</property>
                                        <property name="child">
                                            <object class="AdwClamp">
                                                <property name="maximum-size">1000</property>
                                                <property name="tightening-threshold">600</property>
                                                <property name="child">
                                                    <object class="AdwPreferencesGroup">
                                                        <property name="margin-top">24</property>
                                                        <property name="title" translatable="yes">Create</property>
                                                        <child>
                                                            <object class="GtkBox" id="create_box">
                                                                <property name="halign">center</property>
                                                                <property name="hexpand">False</property>
                                                                <property name="orientation">vertical</property>
                                                                <property name="margin-bottom">36</property>
                                                                <property name="margin-start">12</property>
                                                                <property name="margin-end">12</property>
                                                                <property name="valign">start</property>
                                                            </object>
                                                        </child>
                                                    </object>
                                                </property>
                                            </object>
                                        </property>
                                    </object>
                                </child>
                                <child>
                                    <object class="GtkStackPage">
                                        <property name="name">work</property>
                                        <property name="title" translatable="yes">Work</property>
                                        <property name="icon-name">mail-send-symbolic</property>
                                        <property name="child">
                                            <object class="AdwClamp">
                                                <property name="maximum-size">1000</property>
                                                <property name="tightening-threshold">600</property>
                                                <property name="child">
                                                    <object class="AdwPreferencesGroup">
                                                        <property name="margin-top">24</property>
                                                        <property name="title" translatable="yes">Work</property>
                                                        <child>
                                                            <object class="GtkBox" id="work_box">
                                                                <property name="halign">center</property>
                                                                <property name="hexpand">False</property>
                                                                <property name="orientation">vertical</property>
                                                                <property name="margin-bottom">36</property>
                                                                <property name="margin-start">12</property>
                                                                <property name="margin-end">12</property>
                                                                <property name="valign">start</property>
                                                            </object>
                                                        </child>
                                                    </object>
                                                </property>
                                            </object>
                                        </property>
                                    </object>
                                </child>
                                <child>
                                    <object class="GtkStackPage">
                                        <property name="name">apps</property>
                                        <property name="title" translatable="yes">Apps</property>
                                        <property name="icon-name">view-grid-symbolic</property>
                                        <property name="child">
                                            <object class="AdwClamp">
                                                <property name="maximum-size">1000</property>
                                                <property name="tightening-threshold">600</property>
                                                <property name="child">
                                                    <object class="AdwPreferencesGroup">
                                                        <property name="margin-top">24</property>
                                                        <property name="title" translatable="yes">Apps</property>
                                                        <child>
                                                            <object class="GtkBox" id="apps_box">
                                                                <property name="halign">center</property>
                                                                <property name="hexpand">False</property>
                                                                <property name="orientation">vertical</property>
                                                                <property name="margin-bottom">36</property>
                                                                <property name="margin-start">12</property>
                                                                <property name="margin-end">12</property>
                                                                <property name="valign">start</property>
                                                            </object>
                                                        </child>
                                                    </object>
                                                </property>
                                            </object>
                                        </property>
                                    </object>
                                </child>
                            </object>
                        </child>
                    </object>
                </child>

                <child>
                    <object class="GtkBox" id="leaflet_expanded">
                        <child>
                            <object class="GtkLabel">
                                <property name="label">Label UwU</property>
                            </object>
                        </child>
                    </object>
                </child>
            </object>
        </child>
    </template>
</interface>
